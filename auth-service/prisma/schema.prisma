datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

model User {
  id        String  @id @db.ObjectId @map("_id")
  username  String  @unique
  email     String  @unique
  password  String?
}
